{
  "name": "ci-env-dotnet",
  "description": "Repository supporting gnosian/ci-env-dotnet Docker image.",
  "title": "ci-env-dotnet",
  "version": "6.0.0",
  "ciEnvironment": {
    "variables": [
      {
        "description": "Docker image repository",
        "name": "DOCKER_IMAGE_REPOSITORY",
        "required": true,
        "secret": false
      },
      {
        "description": "Docker username",
        "name": "DOCKER_USERNAME",
        "required": true,
        "secret": false
      },
      {
        "description": "Docker password or API token",
        "name": "DOCKER_PASSWORD",
        "required": true,
        "secret": true
      },
      {
        "description": "A Boolean value indicating whether the Docker buildx context should be maintained between invocations.",
        "name": "DOCKER_USE_PERSISTENT_BUILDX_CONTEXT",
        "required": false,
        "secret": false
      }
    ]
  }
}